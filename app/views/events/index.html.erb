<div class="contents ">

<!-- 検索 -->
  <div class="search">
    <p>エリア検索</p>
    <%= form_tag(search_path, method: :get ) do %>
      <%= text_field_tag :keyword %>
      <%= submit_tag 'Search', :name => nil %>
    <% end %>
  </div>

  <div class="main">

    <%= render @events %>

    <%= paginate(@events) %>
  </div>
</div>